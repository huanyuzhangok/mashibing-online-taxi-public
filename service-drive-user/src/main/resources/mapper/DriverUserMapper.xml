<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mashing.serviceDriverUser.mapper.DriverUserMapper">
    <select id="select1" parameterType="java.lang.String" resultType="java.lang.Integer">
        select count(*)
        from v_ctity_driver_user_work_status
        where city_code = #{cityCode}
    </select>
</mapper>
